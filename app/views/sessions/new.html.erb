<section class="users-new">

  <header class="page-header">
    <h1>Sign Up Here:</h1>
  </header>

  <%= form_for @user do |f| %>
    <%= label_tag "Email" %>
    <%= text_field_tag :email, nil, placeholder: "Email" %>
    <%= label_tag "Password" %>
    <%= password_field_tag :password, nil, placeholder: "Password" %>
    <%= submit_tag "Log In"%>
  <% end %>

</section>